<?xml version='1.0' encoding='ISO-8859-1'?>

<!DOCTYPE swatch SYSTEM "swatch.dtd">

<swatch             name="WWW_LINK_REF">

<!-- Base Satellite definition -->
	<sat	        shape="edgeShp"
					isBack="true">
		<slice		inCol="37 137 235"
					outCol="0 102 204"/> 
	</sat>

<!-- General purpose Selection -->
<!-- Sel sats must be declared before the ones they should hides when they'll be activated -->
	<sat	        shape="edgeShp"
					linkDarkCol="128 0 0"
					linkNormCol="255 0 0"
					linkLitCol="255 143 143"
					selection="search"
					transfo="noTrf">
		<slice		inCol="255 0 0"/>
	</sat>

<!-- References -->
	<shape		    id="edgeShp"
					scale="1">
	</shape>
	<transfo	    id="noTrf"/>

</swatch>

